/**
 * 16-bit incrementer:
 * out = in + 1
 */
CHIP CS21B001Inc16 {
    IN in[16];
    OUT out[16];

    PARTS:
    //// Replace this comment with your code.
    CS21B001Add16(a=in, b[0]=true, out=out);
}

/**
 * 16-bit Or gate:
 * for i = 0, ..., 15:
 * out[i] = a[i] Or b[i] 
 */
CHIP CS21B001Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    	CS21B001Or8(a=a[0..7], b=b[0..7], out=out[0..7]);
        CS21B001Or8(a=a[8..15], b=b[8..15], out=out[8..15]);
}

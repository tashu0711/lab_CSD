/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP CS21B001DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
	    CS21B001Not(in=sel, out=notsel);
	    CS21B001And(a=in, b=notsel, out=a);
	    CS21B001And(a=in, b=sel, out=b);
}
